{
    "name": "ovni-utils",
    "version": "0.2.0",
    "description": "Specific useful functions.",
    "keywords": [
        "ovni",
        "ovniutils",
        "ovni-utils",
        "ovniutilities",
        "ovni-utilities",
        "utilities",
        "utils"
    ],
    "license": "MIT",
    "author": {
        "name": "Lucas O. S.",
        "email": "hi@ovni.dev",
        "url": "https://ovni.dev"
    },
    "type": "module",
    "main": "dist/ovniutils.js",
    "unpkg": "dist/ovniutils.min.js",
    "jsdelivr": "dist/ovniutils.min.js",
    "browser": "dist/ovniutils.min.js",
    "homepage": "https://github.com/ovniroto/ovni-utils",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ovniroto/ovni-utils.git"
    },
    "files": [
        "src",
        "dist"
    ],
    "scripts": {
        "build": "npm run clean && npm run setup && npm run bundle",
        "setup": "node ./build/setup.js",
        "clean": "rimraf dist",
        "bundle": "npm-run-all bundle:**",
        "bundle:umd": "cross-env NODE_ENV=development rollup -c --bundleConfigAsCjs",
        "bundle:umd:min": "cross-env NODE_ENV=production rollup -c --bundleConfigAsCjs",
        "bundle:clean": "rimraf src/bundle.ts"
    },
    "devDependencies": {
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-node-resolve": "^15.2.1",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.3",
        "@types/node": "^20.6.3",
        "@types/uuid": "^9.0.4",
        "cross-env": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.1",
        "rollup": "^3.29.2",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "uuid": "^9.0.1"
    }
}
